/**
 * Represents an LED smart bulb extending the SmartBulb type.
 */
entity type LEDBulb extends SmartBulb type key "LED_BULB" {
    /**
     * The expected lifespan of the LED bulb in hours.
     */
    lifeSpanInHours: int = 10000

    /**
     * The efficiency factor of the LED bulb.
     */
    efficiencyFactor: double = 0.75
}