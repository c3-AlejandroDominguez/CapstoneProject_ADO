{
    "id": "Status_SmartBulb",
    "name": "Status",
    "description": "Status (1 or 0) indication on or off of the SmartBulb over time",
    "srcType": "SmartBulb",
    "path": "bulbMeasurements",
    "expression": "max(max(normalized.data.status))",
    "cache": {
    "intervals": ["HOUR","DAY", "MONTH"],
      "monthsInPast": 4,
      "monthsInFuture": 1
    }
